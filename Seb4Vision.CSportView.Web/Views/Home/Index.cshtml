@{
    ViewData["Title"] = "Match Facts";
}
<!-- Remove asp rendering
<app asp-prerender-module="ClientApp/dist/main-server">Loading...</app>
-->
<style type="text/css">
    html, body {
        background-color: #006837;
 
    }
    #netballbackground {
        background-color: #018b74;
    }


    .score-eagle {
        background-color: #DCBE22;
    }

    .score-birdie {
        background-color: #C82613;
    }

    .score-par {
        background-color: #666666;
    }

    .score-bogey {
        background-color: black;
    }


    .score-dbl-bogey {
        background-color: #0C64C0;
    }
    .score-stroke {
        display: inline-block;
        width: 17px;
        height: 20px;
        text-align: center;
        padding: 0px 5px;
    }

    .golf-position-index {
        width: 20px;
        background-color: red;
        display: inline-block;
        text-align: center;
        color: white;
    }

    .golf-stats-title {
        color: #fe9c1f;
        display: inline-block;
    }
</style>
<app></app>
<script src="~/dist/vendor.js" asp-append-version="true"></script>
@section scripts {
    <script src="~/dist/main-client.js" asp-append-version="true"></script>
}
